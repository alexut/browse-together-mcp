{
	"imports": {
		"@folder/xdg": "npm:@folder/xdg@^4.0.1",
		"playwright": "npm:playwright@^1.51.1",
		"std/flags": "jsr:@std/flags@0.224.0",
		"std/fs": "jsr:@std/fs@0.224.0",
		"std/path": "jsr:@std/path@0.224.0",
		"std/http-server": "jsr:@std/http-server@0.224.0",
		"zod": "npm:zod@^3.22.4"
	},
	"tasks": {
		"start": "deno run --allow-read --lock=deno.lock cli.ts",
		"proxy": "deno run --allow-read --allow-write --allow-net --allow-run --allow-env --allow-sys browser.ts"
	},
	"fmt": {
		"include": ["*.ts"],
		"lineWidth": 80,
		"indentWidth": 2
	},
	"lock": true
}
